{
  "name": "@opencode/asu-discover",
  "version": "2.0.0",
  "type": "module",
  "description": "Semantic search across ASU GitHub repositories via RAG",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "search": "tsx src/cli.ts search",
    "ask": "tsx src/cli.ts ask",
    "health": "tsx src/cli.ts health",
    "setup": "./scripts/setup.sh",
    "test": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.4.1",
    "commander": "^13.1.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
